    "ttsc": "^0.3.1",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    ".{js,ts,tsx}": "eslint --cache --fix",
    ".{js,ts,tsx,css,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
