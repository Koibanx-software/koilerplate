{
  "name": "koiplate",
  "version": "0.23.8",
  "description": "Gateway de koiplate",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/app.ts | npx pino-pretty",
    "cli": "ts-node-dev --transpile-only -r tsconfig-paths/register src/cli.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --config .prettierrc.json --check \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "prettier:fix": "prettier --write .",
    "format": "yarn prettier:fix && yarn lint:fix",
    "start": "npm run build && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/app.js",
    "test": "jest",
    "prepare": "husky install",
    "release": "standard-version --no-verify"
  },
  "dependencies": {
    "@anatine/zod-openapi": "^1.14.2",
    "@aws-sdk/client-s3": "^3.354.0",
    "@aws-sdk/s3-request-presigner": "^3.354.0",
    "@decorators/di": "^2.0.0",
    "@decorators/express": "^2.9.3",
    "@decorators/express-openapi": "^1.1.2",
    "@koibanx/build-sdk": "^0.1.35",
    "@ts-rest/core": "^3.19.3",
    "@ts-rest/express": "^3.19.3",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/figlet": "^1.5.6",
    "@types/jsonwebtoken": "^9.0.2",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "chalk": "^5.2.0",
    "commander": "^10.0.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.0",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "express-list-endpoints": "^6.0.0",
    "express-zod-api": "^11.0.0",
    "figlet": "^1.6.0",
    "jest": "^29.5.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "lint-staged": "^13.0.3",
    "minimist": "^1.2.5",
    "mongo": "^0.1.0",
    "mongodb": "^5.7.0",
    "mongoose": "^7.0.5",
    "mongoose-paginate-v2": "^1.4.2",
    "mongoose-unique-validator": "^3.0.0",
    "mrm": "2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.0",
    "openapi3-ts": "^2.0.0",
    "path": "^0.12.7",
    "pino": "^8.8.0",
    "pino-http": "^8.3.2",
    "pino-pretty": "^9.1.1",
    "prisma": "^4.13.0",
    "purify-ts": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "swagger-ui-express": "^4.6.3",
    "ts-jest": "^29.1.0",
    "tsc-alias": "^1.8.6",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "zod": "^3.21.4"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:koibanx/galmaco/koiplate-gateway.git"
  },
  "author": "Koibanx",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
