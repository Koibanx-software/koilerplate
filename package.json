{
  "name": "koilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:Koibanx-software/koilerplate.git",
  "author": "Arturo GUerrero <arturo.guerrero@koibanx.com>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts | npx pino-pretty",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --config .prettierrc.json --check \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "prettier:fix": "prettier --write .",
    "format": "yarn prettier:fix && yarn lint:fix",
    "start": "npm run build && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/app.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.2",
    "husky": "^8.0.3",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.5"
  },
  "dependencies": {
    "@koibanx/build-sdk": "^0.1.35",
    "@types/jest": "^29.5.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "apidoc": "^1.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "jwt-decode": "^3.1.2",
    "mongo": "^0.1.0",
    "mongodb": "^5.6.0",
    "mongoose": "^7.3.1",
    "pino": "^8.14.1",
    "pino-logger": "^1.0.0",
    "prettier": "^2.8.8",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.21.4"
  }
}
